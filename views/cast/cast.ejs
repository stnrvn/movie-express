<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <a href="/cast/add">add</a>
    <table border="1">
        <thead>
            <tr>
                <td>Name</td>
                <td>Birth Year</td>
                <td>Phone Number</td>
                <td>Gender</td>
                <td>Action</td>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(element => { %>
            
            <tr>
                <td><%= element.fullName() %></td>
                <td><%= element.cast_birthYear %></td>
                <td><%= element.cast_phoneNumber %></td>
                <td><%= element.cast_gender %></td>
                <td><a href="/cast/edit/<%= element.id %>">Edit</a> || 
                    <a href="/cast/delete/<%= element.id %>" onclick=" confirm('yakin ingin dihapus?')">Delete</a> || 
                    <a href="/cast/listmovie/<%= element.id %>">See Movies</a></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>